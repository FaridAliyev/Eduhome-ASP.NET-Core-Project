@model Speaker
@{
    ViewData["Title"] = "Speaker Details";
}

    <div class="single-speaker">
        <div class="speaker-img">
            <img src="~/img/event/@Model.Image" alt="speaker">
        </div>
        <div class="speaker-name">
            <h5>@Model.Fullname</h5>
            <p>@Model.Duty, @Model.Company </p>
        </div>
    </div>
    <a asp-action="Index" class="btn btn-dark btn-icon-text">
        Go Back
        <i class="mdi mdi-keyboard-backspace btn-icon-append"></i>
    </a>

