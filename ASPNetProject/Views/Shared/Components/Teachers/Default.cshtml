@model IEnumerable<TeacherVM>

@foreach (TeacherVM item in Model)
{
    <div class="col-md-3 col-sm-4 col-xs-12">
        <div class="single-teacher mb-45">
            <div class="single-teacher-img">
                <a asp-controller="Teacher" asp-action="Details" asp-route-id="@item.Id"><img src="img/teacher/@item.Image" alt="teacher"></a>
            </div>
            <div class="single-teacher-content text-center">
                <h2><a asp-controller="Teacher" asp-action="Details" asp-route-id="@item.Id">@item.Fullname</a></h2>
                <h4>@item.TeacherInfo.Qualification</h4>
                <ul>
                    <li><a href="@item.TeacherInfo.Facebook"><i class="zmdi zmdi-facebook"></i></a></li>
                    <li><a href="@item.TeacherInfo.Pinterest"><i class="zmdi zmdi-pinterest"></i></a></li>
                    <li><a href="@item.TeacherInfo.Vimeo"><i class="zmdi zmdi-vimeo"></i></a></li>
                    <li><a href="@item.TeacherInfo.Twitter"><i class="zmdi zmdi-twitter"></i></a></li>
                </ul>
            </div>
        </div>
    </div>
}